{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env process.env.section=admin NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mobile-dev": "npm run mobile-development",
        "mobile-development": "cross-env process.env.section=mobile NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mobile-watch": "cross-env process.env.section=mobile NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mobile-prod": "npm run mobile-production",
        "mobile-production": "cross-env process.env.section=mobile NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "all-prod": "npm run prod && npm run mobile-prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "babel-preset-env": "^1.7.0",
        "bootstrap-sass": "^3.3.7",
        "cross-env": "^5.1",
        "element-ui": "^2.15.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^4.0.7",
        "localforage": "^1.7.2",
        "lodash": "^4.17.13",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.6",
        "vue-cropper": "^0.5.6",
        "vue-template-compiler": "^2.6.11",
        "vuex": "^3.5.1"
    },
    "dependencies": {
        "driver.js": "^0.9.8",
